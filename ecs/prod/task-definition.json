{
  "family": "team6-monew-prod-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::366080762812:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::366080762812:role/team6-monew-task-rules",
  "containerDefinitions": [
    {
      "name": "team6-monew-prod-container",
      "image": "<IMAGE_PLACEHOLDER>",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DB_URL",
          "value": "REPLACEME_DB_URL"
        },
        {
          "name": "DB_USERNAME",
          "value": "REPLACEME_DB_USERNAME"
        },
        {
          "name": "DB_PASSWORD",
          "value": "REPLACEME_DB_PASSWORD"
        },
        {
          "name": "MONGO_URI",
          "value": "REPLACEME_MONGO_URI"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        },
        {
          "name": "AWS_S3_ACCESS_KEY_ID",
          "value": "id"
        },
        {
          "name": "AWS_S3_SECRET_ACCESS_KEY",
          "value": "pw"
        },
        {
          "name": "AWS_REGION",
          "value": "REGION"
        },
        {
          "name": "AWS_BUCKET",
          "value": "BUCKET"
        },
        {
          "name": "NAVER_CLIENT_ID",
          "value": "ID"
        },
        {
          "name": "NAVER_CLIENT_SECRET",
          "value": "SECRET"
        },
        {
          "name": "AWS_BACKUP_BUCKET",
          "value": "AWS_BACKUP_BUCKET"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/team6-monew-prod-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
